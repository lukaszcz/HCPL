
// simple arithmetic test

let f = \x \y {
   print (+ x y);
};

let g = \x \y {
   - (+ x 1) (* y 7)
};

let h = \f \y \z {
   * (f y y) z
};

(f (h g 2 3) 4); // (g 2 2) * 3 + 4 = (2+1-2*7)*3 + 4 = -11*3 + 4 = -33+4 = -29

